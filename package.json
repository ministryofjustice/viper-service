{
  "name": "viper-service",
  "description": "A prototype service for providing access to VIPER Ratings",
  "version": "0.0.1",
  "private": true,
  "author": "Matt Smith <mattonfoot@gmail.com> (http://about.me/mattonfoot)",
  "license": "MIT",
  "engines": {
    "npm": "3.10.10",
    "node": "6.10.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js | ./node_modules/.bin/bunyan -o short",
    "test": "./node_modules/mocha/bin/mocha ./test/**/**.js ./test/**.js",
    "test-report": "./node_modules/mocha/bin/mocha ./test/**/**.js ./test/**.js --reporter mocha-junit-reporter",
    "test-swagger": "./node_modules/swagger/bin/swagger.js project test",
    "verify": "./node_modules/swagger/bin/swagger.js project verify",
    "docs-test": "./node_modules/dredd/bin/dredd ./api/swagger/swagger.yaml http://127.0.0.1:3000",
    "docs-render": "./node_modules/spectacle-docs/bin/spectacle.js -t ./docs ./api/swagger/swagger.yaml "
  },
  "dependencies": {
    "bunyan": "1.8.10",
    "require-sql": "1.0.0",
    "restify": "4.3.0",
    "swagger-restify-mw": "0.1.0",
    "tedious": "2.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "dredd": "3.4.5",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.13.0",
    "spectacle-docs": "0.8.2",
    "supertest": "3.0.0",
    "swagger": "0.7.5"
  }
}
