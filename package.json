{
  "name": "viper-service",
  "description": "A prototype service for providing access to VIPER Ratings",
  "version": "0.0.1",
  "private": true,
  "author": "Matt Smith <mattonfoot@gmail.com> (http://about.me/mattonfoot)",
  "license": "GPL-3.0",
  "engines": {
    "npm": "3.10.10",
    "node": "6.10.0"
  },
  "scripts": {
    "start": "node server.js | ./node_modules/.bin/bunyan -o short",
    "test": "mocha ./tests/**/**.js ./tests/**.js",
    "test-report": "mocha ./tests/**/**.js ./tests/**.js --reporter mocha-junit-reporter",
    "docs-test": "./node_modules/dredd/bin/dredd",
    "docs-render": "./node_modules/aglio/bin/aglio.js --theme-variables slate --theme-template triple -i docs/docs.apib -o docs/index.htm",
    "docs-convert": "./node_modules/apib2swagger/bin/apib2swagger.js -i docs/docs.apib -o docs/resources.json"
  },
  "dependencies": {
    "bunyan": "1.8.10",
    "crypto": "0.0.3",
    "node-restify-swagger": "0.1.8",
    "node-restify-validation": "1.1.1",
    "require-sql": "1.0.0",
    "restify": "4.3.0",
    "swagger-express-mw": "0.7.0",
    "swagger-ui": "2.2.10",
    "tedious": "2.0.0"
  },
  "devDependencies": {
    "aglio": "2.3.0",
    "apib2swagger": "0.8.0",
    "chai": "3.5.0",
    "dredd": "^3.4.5",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.13.0",
    "supertest": "3.0.0",
    "swagger": "0.7.5"
  }
}
