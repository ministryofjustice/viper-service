{
  "name": "viper-service",
  "description": "A Restful service to provide access to the VIPER Rating for a known ID",
  "version": "0.0.1",
  "private": true,
  "author": "Matt Smith <mattonfoot@gmail.com> (http://about.me/mattonfoot)",
  "license": "MIT",
  "engines": {
    "npm": "3.10.10",
    "node": "6.10.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js | bunyan -o short",
    "test": "mocha",
    "test-report": "mocha --reporter mocha-junit-reporter",
    "verify": "swagger project verify",
    "docs-test": "dredd ./api/swagger/swagger.yaml http://127.0.0.1:3000",
    "docs-render": "spectacle -t ./docs ./api/swagger/swagger.yaml",
    "setup-mssql": "node ./bin/setup-mssql",
    "knex": "knex",
    "knex-migrate": "knex-migrate",
    "migrate": "knex migrate:latest",
    "migrate-dry": "knex-migrate pending",
    "seed": "knex seed:run",
    "record-build-info": "node ./bin/record-build-info",
    "wait-for-deploy": "node ./bin/wait-for-deploy"
  },
  "dependencies": {
    "bunyan": "1.8.10",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "mssql": "^3.3.0",
    "require-sql": "1.0.0",
    "restify": "^4.3.1",
    "sleep-promise": "^2.0.0",
    "swagger-restify-mw": "^0.7.0",
    "tedious": "2.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "date-fns": "^1.28.5",
    "dredd": "^4.1.1",
    "knex-migrate": "^1.3.0",
    "mocha": "^3.4.2",
    "mocha-junit-reporter": "1.13.0",
    "mock-knex": "^0.3.9",
    "sinon": "^2.3.5",
    "spectacle-docs": "0.8.2",
    "supertest": "3.0.0",
    "swagger": "0.7.5",
    "wait-port": "^0.1.4"
  }
}
